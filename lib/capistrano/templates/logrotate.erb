<%= fetch(:logrotate_log_path) %>/*.log {
    <%= fetch(:logrotate_interval) %>
    rotate <%= fetch(:logrotate_logs_keep) %>

    missingok
    compress
    delaycompress
    notifempty
    copytruncate
    dateext dateformat -%Y%m%d%H

    su <%= fetch(:logrotate_user) %> <%= fetch(:logrotate_group) %>
}
